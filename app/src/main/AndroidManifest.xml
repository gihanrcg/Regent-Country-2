<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gisilk.onlineorder2">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".com.functions.signUp.SignupActivity" />
        <activity android:name=".com.functions.signUp.ResetPasswordActivity" />
        <activity android:name=".com.functions.UserProfile.UserProfileActivity" />
        <activity android:name=".com.functions.Booking.Booking" />
        <activity android:name=".com.functions.Admin.AdminPanel" />
        <activity
            android:name=".com.functions.Home.HomePage"
            android:label="@string/title_activity_home_page"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity android:name=".com.functions.Booking.MyBookings">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".com.functions.OrderFood.OrderFood"></activity>
        <activity android:name=".com.functions.Admin.AddLiquor"></activity>
        <activity android:name=".com.functions.Admin.AddFood"></activity>
        <!--<activity android:name=".com.functions.Booking.MyBookings"></activity>-->
    </application>

</manifest>